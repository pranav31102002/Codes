def fib_rec(n):
    if n <= 1:
        return n
    return fib_rec(n-1) + fib_rec(n-2)

def fib_nonrec(n):
    a, b = 0, 1
    print(a, b, end=" ")
    for i in range(2, n):
        a, b = b, a + b
        print(b, end=" ")
    print()

n = int(input("Enter number of terms: "))

print("\nRecursive Fibonacci:")
for i in range(n):
    print(fib_rec(i) , end = " ")

print("\n\nNon-Recursive Fibonacci:")
fib_nonrec(n)

