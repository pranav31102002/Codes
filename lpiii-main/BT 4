pragma solidity  ^0.8.0;

contract StudentData
{
    struct student
    {
        uint id;
        string name;
    }
    student[] public st;
    function addstudent(uint id , string memory name)
    public 
    {
        st.push(student(id,name));
    }

    function totalstudents()
    public view returns (uint)
    {
        return st.length;
    }

    fallback() external payable { } 


}
